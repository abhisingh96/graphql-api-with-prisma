{
  "name": "node-graphql-prisma",
  "version": "1.0.0",
  "description": "Here we are creating API's using nodejs, graphql and prisma",
  "main": "./index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets babel-preset-env",
    "start-test-server": "NODE_ENV=test babel-node index.js --presets babel-preset-env",
    "test": "./reset_test_db.sh; ENV=test mocha --require babel-register './test/*.test.js'",
    "coverage": "ENV=test nyc --reporter=html --reporter=text --reporter=lcov npm run test",
    "dev": "nodemon index.js NODE_ENV=dev ENV=dev --exec babel-node --presets babel-preset-env",
    "format": "prettier --write \"./**/*.{js,jsx,json}\""
  },
  "keywords": [
    "graphql",
    "nodejs",
    "prisma",
    "postgres"
  ],
  "author": "Abhishek Singh",
  "license": "ISC",
  "devDependencies": {
    "apollo": "^2.31.2",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-request": "^3.3.0",
    "graphql-tools": "^6.2.4",
    "graphql-type-json": "^0.3.0",
    "http-status-codes": "^2.1.4",
    "pg": "^7.12.1",
    "pg-promise": "^10.3.5",
    "prisma": "^2.25.0",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "server": "^1.0.31",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.7",
    "@prisma/client": "^2.30.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql": "^15.4.0",
    "mocha": "^8.2.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0"
  }
}
